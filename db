CREATE DATABASE roomify;

USE roomify;

CREATE TABLE schedules (
  id INT AUTO_INCREMENT PRIMARY KEY,
  room VARCHAR(50),
  course VARCHAR(100),
  professor VARCHAR(100),
  start_time TIME,
  end_time TIME,
  day VARCHAR(20)
);

CREATE TABLE IF NOT EXISTS schedule (
  id INT AUTO_INCREMENT PRIMARY KEY,
  room VARCHAR(50),
  course VARCHAR(100),
  professor VARCHAR(100),
  start_time TIME,
  end_time TIME,
  day VARCHAR(20)
);


CREATE TABLE rooms (
  id INT AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(100) NOT NULL
);


CREATE DATABASE login;

CREATE TABLE users{
id INT(10) PRIMARY KEY,
fistName VARCHAR(50),
lastName VARCHAR(50),
email VARCHAR(50),
password VARCHAR(50)
);

